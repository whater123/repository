<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="com.springboot.mybatis.dao.UerRegisterMapper">

    <select id="getAllUsers" resultType="User">
       select * from user_register
    </select>

    <select id="getAllAll" resultType="User">
        select * from
        user_register as b left join user_signup as a on a.id = b.id
        left join user_dalao as d on b.id = d.id
    </select>

    <insert id="insertUser" parameterType="User">
        insert into user_register (number,password,id) values (#{number},#{password},'0')
    </insert>

</mapper>